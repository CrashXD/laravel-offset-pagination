{
    "name": "chelout/offset-pagination",
    "description": "Offset pagination made easy for Laravel",
    "homepage": "https://github.com/chelout/offset-pagination",
    "license": "MIT",
    "keywords": ["laravel", "pagination", "api"],
    "authors": [
        {
            "name": "chelout",
            "email": "chelout@gmail.com"
        }
    ],
    "require": {
      "php": ">=7.0.0",
      "illuminate/support": "5.5.*||5.6.*||5.7.*",
      "illuminate/http": "5.5.*||5.6.*||5.7.*",
      "illuminate/database": "5.5.*||5.6.*||5.7.*"
    },
    "require-dev": {
      "orchestra/testbench": "~3.5",
      "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Chelout\\OffsetPagination\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chelout\\OffsetPagination\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chelout\\OffsetPagination\\OffsetPaginationServiceProvider"
            ],
            "aliases": {
                "OffsetPagination": "Chelout\\OffsetPagination\\OffsetPagination"
            }
        }
    }
}
